class Box {
    double width;
    double height;
    double depth;

    Box() {
        width = 1;
        height = 1;
        depth = 1;
    }

    double volume() {
        return width * height * depth;
    }
}

public class BoxVolume {
    public static void main(String[] args) {
        Box myBox = new Box();
        double vol = myBox.volume();
        System.out.println("Volume of the box: " + vol);
    }
}
